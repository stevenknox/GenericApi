@model List<SampleWebApi.Models.Product>
@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
    <div class="col-md-10">
        <h2>Products <small>(Loaded using IGenericRepository injected into HomeController)</small></h2>
        <ul>
            @foreach(var product in @Model)
            {
                <li>@product.Name</li>
            }    
        </ul>
    </div>
</div>
